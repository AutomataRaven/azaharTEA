#:kivy 1.9.1

<MenuBar>:    

    file_menu: file_menu
    see_menu: see_menu
    
    size_hint_x: 1
    size_hint_y: None
    height: 40
    
    canvas:
        Color: 
            rgba: 0.6, 0.63, 0.68, 1
        Rectangle:
            pos: self.pos
            size: self.size        
        
    FileMenu:
        id: file_menu
        
    SeeMenu:
        id: see_menu
